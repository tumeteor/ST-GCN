{
  "queryType":"timeseries",
  "dataSource":"mytaxi-speed-berlin-index",
  "intervals":[
    "2019-04-01/2019-04-07"
  ],
  "granularity":"fifteen_minute",
  "aggregations":[
    {
       "type" : "floatFirst",
       "name" :"from_node",
       "fieldName" :"from_node"
    },
    {
       "type" : "floatFirst",
       "name" :"to_node",
       "fieldName" :"to_node"
    },
    {
      "type":"count",
      "name":"count"
    },
    {
      "type":"doubleSum",
      "name":"total_speed",
      "fieldName":"estimated_speed"
    }
  ],
  "postAggregations" : [{
    "type"   : "arithmetic",
    "name"   : "average",
    "fn"     : "*",
    "fields" : [
      { "type"   : "arithmetic",
        "name"   : "div",
        "fn"     : "quotient",
        "fields" : [
          { "type" : "fieldAccess", "name" : "total_speed", "fieldName" : "total_speed" },
          { "type" : "fieldAccess", "name" : "count", "fieldName" : "count" }

        ]
      },
      { "type" : "constant", "name": "const", "value" : 1 }
    ]
  }],
  "context":{
    "grandTotal":false
  },
  "filter":{
    "type":"and",
    "fields": [
      {
        "type":"selector",
        "dimension":"from_node",
        "value":"387513698"
      },
      {
        "type":"selector",
        "dimension":"to_node",
        "value":"1392640958"
      },
      {
        "type":"bound",
        "dimension":"estimated_speed",
        "lower":"0",
        "lowerStrict": true,
        "upper":"100"
      }
    ]
  }
}
